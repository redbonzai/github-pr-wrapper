{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": "test/e2e",
  "testEnvironment": "node",
  "testRegex": ".*\\.e2e-spec\\.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "collectCoverageFrom": ["**/*.(t|j)s"],
  "coverageDirectory": "../../coverage",
  "testPathIgnorePatterns": [
    "/node_modules/",
    "/dist/"
  ]
}
