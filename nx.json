{
  "npmScope": "nest-pr-wrapper",
  "affected": {
    "defaultBase": "main"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "nx-cloud",
      "options": {
        "cacheableOperations": [
          "build",
          "lint",
          "test",
          "e2e"
        ],
        "heartbeatTimeout": 10000,
        "accessToken": "NX_ACCESS_TOKEN_PLACEHOLDER"
      }
    }
  },
  "targetDefaults": {
    "lint": {
      "inputs": [
        "default",
        "{workspaceRoot}/eslint.config.js"
      ]
    }
  },
  "projects": {
    "nest-pr-wrapper-root": {
      "tags": []
    }
  }
}